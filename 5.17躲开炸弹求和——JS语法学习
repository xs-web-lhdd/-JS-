<script>
        var arr = [1,2,3,4,'炸弹',13,'炸弹',90];
        var sum = 0;
        for(var i = 0; i < arr.length; i++){
            if(typeof arr[i] != 'number'){
                continue;
            }else{
                sum += arr[i];
            }
        }

        console.log(sum);
</script>
